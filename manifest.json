{
    "manifest_version":2,
    "version":"1.0",
    "name":"wach time limitor",
    "content_scripts":[
      {
       "matches":["*://*.youtube.com/*"],
       "js":["main.js"]
      }
    ],
    "options_ui": {
      "page": "options.html"
    },
  
    "permissions": ["storage"],

    "browser_specific_settings": {
      "gecko": {
        "id": "wachTimeLimitor@example.com",
        "strict_min_version": "42.0"
      }
  }
}
  